{
    "manifest_version": 2,
    "name": "Random Joke Generator",
    "version": "1.0",
    "description": "Browser extension that generates random jokes (can be cringe)",
    "author":  "asisamko",
    "icons": {
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "/icons/icon48.png"
    },
    "permissions": [
      "https://icanhazdadjoke.com/*"
    ]
}